   "source": [
    "n = int(input(\"enter the number:\"))\n",
    "sum = 0\n",
    "i = 0\n",
    "while i <= n:\n",
    "    sum = sum + i\n",
    "    i = i + 1\n",
    "    print(i , \"-->\", sum)\n",
    "print(\"sum of numbers is\", sum)"
   ]
